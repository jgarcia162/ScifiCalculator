<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <TableRow
        android:id="@+id/tableRow0">

    <TextView
        android:id="@+id/landdisplay"
        android:padding="5dp"
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="75dp" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow1">

    <Button
        android:id="@+id/cos"
        android:text="@string/cos"
        style="?android:attr/borderlessButtonStyle"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="#39add1"
        android:padding="5dp"
        android:layout_weight="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/sin"
        android:text="@string/sin"
        style="?android:attr/borderlessButtonStyle"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="#3079ab"
        android:padding="5dp"
        android:layout_weight="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/tan"
        android:text="@string/tan"
        style="?android:attr/borderlessButtonStyle"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="#c25975"
        android:padding="5dp"
        android:layout_weight="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

        <Button
            android:id="@+id/multiple"
            android:textAllCaps="false"
            android:padding="5dp"
            android:text="x"
            style="?android:attr/borderlessButtonStyle"
            android:textSize="25sp"
            android:textStyle="bold"
            android:textColor="#e15258"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/divide"
            android:padding="5dp"
            android:text="\u00F7"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#f9845b" />

        <Button
            android:id="@+id/plus"
            android:padding="5dp"
            android:text="+"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#838cc7"/>

        <Button
            android:id="@+id/minus"
            android:padding="5dp"
            android:text="-"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#7d669e"/>
    </TableRow>

    <TableRow
        android:id="@+id/tableRow2">

    <Button
        android:id="@+id/square"
        android:text="√"
        android:textSize="25sp"
        android:textStyle="bold"
        android:padding="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:textColor="#53bbb4"
        style="?android:attr/borderlessButtonStyle" />

    <Button
        android:id="@+id/ln"
        android:text="@string/ln"
        android:textSize="20sp"
        android:textStyle="bold"
        android:padding="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:textColor="#51b46d"
        style="?android:attr/borderlessButtonStyle"/>

     <Button
         android:id="@+id/log"
         android:text="@string/log"
         android:textSize="20sp"
         android:textStyle="bold"
         android:padding="5dp"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_weight="1"
         android:textColor="#e0ab18"
         style="?android:attr/borderlessButtonStyle"/>

     <Button
        android:id="@+id/number7"
        android:padding="5dp"
        android:text="7"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#637a91" />


        <Button
            android:id="@+id/number8"
            android:padding="5dp"
            android:text="8"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#f092b0" />

        <Button
            android:id="@+id/number9"
            android:padding="5dp"
            android:text="9"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#b7c0c7" />

        <Button
            android:id="@+id/delete"
            android:padding="5dp"
            android:text="D"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#39add1" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow3">

    <Button
        android:id="@+id/pi"
        android:text="π"
        android:textSize="25sp"
        android:textStyle="bold"
        android:padding="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#3079ab"
        android:layout_weight="1"
        style="?android:attr/borderlessButtonStyle"/>

    <Button
        android:id="@+id/e"
        android:textAllCaps="false"
        android:text="e"
        android:textSize="25sp"
        android:textStyle="bold"
        android:padding="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#c25975"
        android:layout_weight="1"
        style="?android:attr/borderlessButtonStyle"/>

    <Button
        android:id="@+id/rad"
        android:text="@string/rad"
        android:textSize="20sp"
        android:textStyle="bold"
        android:padding="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#e15258"
        android:layout_weight="1"
        style="?android:attr/borderlessButtonStyle"/>

    <Button
        android:id="@+id/number4"
        android:padding="5dp"
        android:text="4"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_weight="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#f9845b" />

        <Button
            android:id="@+id/number5"
            android:padding="5dp"
            android:text="5"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#838cc7" />

        <Button
            android:id="@+id/number6"
            android:padding="5dp"
            android:text="6"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#7d669e" />

        <Button
            android:id="@+id/clear"
            android:layout_weight="1"
            android:padding="5dp"
            android:text="C"
            android:textSize="25sp"
            android:layout_margin="5dp"
            style="?android:attr/borderlessButtonStyle"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textColor="#c25975"/>

    </TableRow>

    <TableRow
        android:id="@+id/tableRow4">

    <Button
        android:id="@+id/BigE"
        android:text="E"
        android:padding="5dp"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_weight="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#53bbb4"/>

    <Button
        android:id="@+id/openpara"
        android:text="("
        android:padding="5dp"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_weight="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#51b46d" />

    <Button
        android:id="@+id/endpara"
        android:text=")"
        android:padding="5dp"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_weight="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#e0ab18" />

        <Button
            android:id="@+id/number1"
            android:padding="5dp"
            android:text="1"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#637a91" />

        <Button
            android:id="@+id/number2"
            android:padding="5dp"
            android:text="2"
            android:textSize="25dp"
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#f092b0"/>

        <Button
            android:id="@+id/number3"
            android:padding="5dp"
            android:text="3"
            android:textSize="25sp"
            android:textStyle="bold"
            android:background="#00000000"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#b7c0c7"/>

        <Button
            android:id="@+id/percent"
            android:padding="5dp"
            android:text="%"
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#39add1"/>

    </TableRow>

    <TableRow
        android:id="@+id/tableRow5">
        <Button
            android:id="@+id/cosh"
            android:text="@string/cosh"
            style="?android:attr/borderlessButtonStyle"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#3079ab"
            android:padding="5dp"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <Button
            android:id="@+id/sinh"
            android:text="@string/sinh"
            style="?android:attr/borderlessButtonStyle"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#c25975"
            android:padding="5dp"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/tanh"
            android:text="@string/tanh"
            style="?android:attr/borderlessButtonStyle"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#e15258"
            android:padding="5dp"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <Button
            android:id="@+id/zero"
            android:padding="5dp"
            android:text="0"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#f9845b"/>

        <Button
            android:id="@+id/period"
            android:padding="5dp"
            android:text="."
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#838cc7"/>

        <Button
            android:id="@+id/negative"
            android:padding="5dp"
            android:text="(-)"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#7d669e"/>

        <Button
            android:id="@+id/equal"
            android:padding="5dp"
            android:text="="
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="#53bbb4"/>

    </TableRow>


</TableLayout>
